{
    "name": "tomatophp/filament-ecommerce",
    "type": "library",
    "description": "Build your own ecommerce store with FilamentPHP with the Power of Tomato CMS Builder",
    "keywords": [
        "php",
        "laravel",
        "ecommerce",
        "filament",
        "splade",
        "tomatophp",
        "prdoucts",
        "orders",
        "cart"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\FilamentEcommerce\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\FilamentEcommerce\\FilamentEcommerceServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "tomatophp/console-helpers": "^1.1",
        "filament/filament": "^3.0.0",
        "milon/barcode": "^11.0",
        "flowframe/laravel-trend": "^0.2.0",
        "tomatophp/filament-cms": "^v1.0",
        "tomatophp/filament-locations": "^v1.0",
        "tomatophp/filament-accounts": "^v2.0",
        "tomatophp/filament-types": "^v1.0",
        "tomatophp/filament-settings-hub": "^v1.0"
    }
}
